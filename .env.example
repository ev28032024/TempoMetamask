# AdsPower Settings
ADSPOWER_API_URL=http://local.adspower.net:50325
ADSPOWER_API_KEY=

# Google Sheets Settings
GOOGLE_SHEET_ID=your_sheet_id_here
GOOGLE_CREDENTIALS_PATH=credentials.json

# Sheet Column Configuration (0-indexed)
SHEET_SERIAL_NUMBER_COL=0
SHEET_STATUS_COL=1
SHEET_TIMESTAMP_COL=2
SHEET_NAME=Sheet1

# MetaMask Password Pattern
# Password will be: METAMASK_PASSWORD_PREFIX + serial_number
METAMASK_PASSWORD_PREFIX=ВвожуПароль!

# Parallelism
MAX_PARALLEL_PROFILES=1

# Timeouts (seconds)
PAGE_LOAD_TIMEOUT=30
ELEMENT_WAIT_TIMEOUT=15
TRANSACTION_TIMEOUT=60
